{
  "extends": "stylelint-config-standard",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "include",
        "mixin",
        "if",
        "else",
        "function",
        "extend",
        "each",
        "at-root",
        "debug",
        "warn",
        "error"
      ]
    }]
  }
}
